{
  "name": "stylized-tellraw-editor",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "main.js",
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/themes": "^3.2.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.6",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "prismjs": "^1.30.0",
    "re-resizable": "^6.11.2",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.14.1",
    "react-split": "^2.0.14",
    "reactflow": "^11.11.4",
    "slate": "^0.114.0",
    "slate-history": "^0.113.1",
    "slate-react": "^0.114.2",
    "tailwind-merge": "^3.3.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "start": "ELECTRON_START_URL=http://localhost:3000 concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && electron --ozone-platform-hint=auto .\"",
    "build": "react-scripts build",
    "dist": "electron-builder --win nsis zip",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.marcusbandit.easy-tellraw",
    "productName": "Easy Tellraw",
    "files": [
      "build/**/*",
      "main.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icon.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@types/prismjs": "^1.26.5",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.3.1",
    "electron-builder": "^24.6.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.8",
    "wait-on": "^8.0.3"
  }
}
